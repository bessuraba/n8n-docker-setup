{
  "name": "n8n-docker-setup",
  "version": "1.0.0",
  "description": "Docker setup for n8n with custom nodes support",
  "main": "index.js",
  "scripts": {
    "start": "docker compose up -d",
    "stop": "docker compose down",
    "restart": "docker compose restart",
    "logs": "docker compose logs -f",
    "build": "docker compose build",
    "clean": "docker compose down -v && docker system prune -f",
    "dev:install": "./scripts/install-nodes.sh",
    "dev:discover": "./scripts/discover-nodes.sh --auto-discover",
    "dev:create-node": "./scripts/create-node-repo.sh",
    "dev:manage-repos": "./scripts/manage-node-repos.sh",
    "dev:publish": "./scripts/publish-nodes.sh",
    "dev:deploy": "./scripts/deploy-vps.sh",
    "docker:install": "./scripts/docker-entrypoint.sh"
  },
